<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Profile
			{% endblock %}
		</title>
		{# Run `composer require symfony/webpack-encore-bundle`
																																           and uncomment the following Encore helpers to start using Symfony UX #}
		{% block stylesheets %}
			<link rel="stylesheet" href="css/style.css">

			{#<link rel="stylesheet" href="css/bootstrap.min.css">#}

			{#{{ encore_entry_link_tags('app') }}#}
			<!-- CSS only -->
			<!-- CSS only -->
			{#
																																										 <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x" crossorigin="anonymous"> 
																																										#}
			<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.12.1/css/all.css" crossorigin="anonymous"> <link
			href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
			<!-- Axentix CSS minified version -->
			<link
			rel="stylesheet" href="https://cdn.jsdelivr.net/npm/axentix@1.3.2/dist/css/axentix.min.css">
			<!-- Grix system -->
			<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/axentix@1.3.2/dist/css/grix.min.css">


		{% endblock %}


	</head>
	<body>

		<header>
			<nav class="navbar navbar-expand-lg navbar-dark bg-primary fixed-top">
				<div class="container-fluid">
					<a class="navbar-brand" href="#"><img src="{{asset('images/logo.jpeg')}}" alt="logo" width="60x" height="30px" srcset=""></a>
					<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
						<span class="navbar-toggler-icon"></span>
					</button>

					<div class="collapse navbar-collapse" id="navbarColor01">
						<ul class="navbar-nav me-auto">
							<li class="nav-item active">
								<a class="nav-link" href="{{ path('app_home')}}">Kay Deuk
									<span class="visually-hidden">(current)</span>
								</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" href="{{ path('app_annouce_index')}}">Annonces</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" href="{{path('app_contact')}}">Contact</a>
							</li>
							{% if app.user %}
								{% if is_granted('ROLE_ADMIN') %}
									<li class="nav-item">
										<a class="nav-link" href="{{path('app_admin')}}">Gestion Administration</a>
									</li>
								{% endif %}
							{% endif %}


							<li class="nav-item">
								<a class="nav-link" href="{{path('app_login')}}">Publier  une Annonce</a>
							</li>
							{% if app.user %}
								<li class="nav-item">
									<a class="nav-link" href="{{path('app_logout')}}">Deconnexion</a>
								</li>
						{% else %}


							</ul>
							<ul class="navbar-nav ml-auto">
								<li class="nav-item">
									<a class="nav-link" href="{{path('app_register')}}">Inscription</a>
								</li>
								<li class="nav-item">
									<a class="nav-link" href="{{path('app_login')}}">Connexion</a>
								</li>
							</ul>
						{% endif %}

					</div>
				</div>
			</nav>
		</header>
		<main class="mt-5">
			<div id="example-sidenav" data-ax="sidenav" class="sidenav shadow-1 large fixed white">
				<div class="sidenav-header">
					<button data-target="example-sidenav" class="sidenav-trigger">
						<i class="fas fa-times"></i>
					</button>
					<img class="sidenav-logo dropshadow-1" src="your_img_path" alt="Logo"/>

				</div>
				<a href="#" class="sidenav-link active">Mon profil</a>
				<a href="#" class="sidenav-link">Mes Annonces</a>

			</div>
			<main>
				<button data-target="example-sidenav" class="btn rounded-1 press amaranth dark-1 sidenav-trigger hide-md-up">
					Open sidenav
				</button>
			</main>

			{% block body %}{% endblock %}
		</main>


		<footer class="mt-5 bg-light py-4 text-center">
			<p>Copyright &copy simplon.co !!</p>
		</footer>
		<script src="https://cdn.jsdelivr.net/npm/axentix@1.3.2/dist/js/axentix.min.js"></script>

		{% block javascripts %}
			{#{{ encore_entry_script_tags('app') }}#}
			<!-- JavaScript Bundle with Popper -->
			<!-- JavaScript Bundle with Popper --><script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"> </script>
			<link
			href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">

			<!-- Axentix JS minified version -->
			<script src="https://cdn.jsdelivr.net/npm/axentix@1.3.2/dist/js/axentix.min.js"></script>
		{% endblock %}
		<script>
			var sidenav = new Sidenav('#example-sidenav');
		</script>
	</body>
</html>
